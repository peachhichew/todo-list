{
  "name": "todo-list",
  "version": "1.0.0",
  "description": "This is todo list app that uses a web API.",
  "main": "server.js",
  "scripts": {
    "build": "babel ./client --out-file ./hosted/bundle.js",
    "watch": "babel --watch ./client --out-file ./hosted/bundle.js",
    "nodemon": "nodemon --watch ./src  --watch ./hosted ./src/server.js",
    "start": "node ./src/server.js",
    "pretest": "eslint ./src --fix",
    "test": "echo \"Tests complete\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peachhichew/todo-list.git"
  },
