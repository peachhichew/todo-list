  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/peachhichew/todo-list/issues"
  },
  "homepage": "https://github.com/peachhichew/todo-list#readme",
  "dependencies": {
    "eslint": "^6.5.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "nodemon": "^1.19.3"
  }
}
